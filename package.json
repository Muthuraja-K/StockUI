{
  "name": "stock-update",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --configuration development",
    "start:prod": "ng serve --configuration production",
    "build": "ng build",
    "build:dev": "ng build --configuration development",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "clean": "rimraf \"../StockWebApi/static\" && mkdir \"../StockWebApi/static\"",
    "clean:win": "if exist \"..\\StockWebApi\\static\" rmdir /s /q \"..\\StockWebApi\\static\" && mkdir \"..\\StockWebApi\\static\"",
    "copytoapi": "xcopy /E /I /Y dist\\StockUpdate\\browser\\* ..\\StockWebApi\\static\\",
    "copy-to-api-unix": "copyfiles -f dist/StockUpdate/browser/* ../StockWebApi/static/",
    "copy-api-to-d": "xcopy /E /I /Y /EXCLUDE:copy-exclude.txt ..\\StockWebApi\\* D:\\friendfortpolio\\",
    "copy-api-to-d-unix": "copyfiles -f ../StockWebApi/* D:/friendfortpolio/ --exclude=**/__pycache__/** --exclude=**/.vscode/**",
    "build-and-copy": "npm run clean && npm run build:dev && npm run copy-to-api",
    "build-prod-and-copy": "npm run clean && npm run build:prod && npm run copy-to-api",
    "deploy": "npm run clean && npm run build:prod && npm run copytoapi",
    "deployprod": "npm run clean && npm run build:prod && npm run copytoapi && npm run copy-api-to-d"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.1.3",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/material": "^20.1.3",
    "@angular/material-moment-adapter": "^20.1.3",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@ng-select/ng-select": "^15.1.3",
    "@types/chart.js": "^2.9.41",
    "chart.js": "^4.5.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.1",
    "@angular/cli": "^20.1.1",
    "@angular/compiler-cli": "^20.1.0",
    "@types/jasmine": "~5.1.0",
    "copyfiles": "^2.4.1",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "rimraf": "^5.0.5",
    "typescript": "~5.8.2"
  }
}
